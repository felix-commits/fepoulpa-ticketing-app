{
  "expo": {
    "name": "ticket-app",
    "slug": "ticket-app",
    "version": "2.2.1",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "backgroundColor": "#ddffe2",
      "image": "./src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "versionCode": 38
    },
    "androidNavigationBar": {
      "backgroundColor": "#ddffe2",
      "barStyle": "dark-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#ddffe2",
      "barStyle": "dark-content"
    },
    "plugins": [
      "expo-dev-client"
    ]
  }
}
